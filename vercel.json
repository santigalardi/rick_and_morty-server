{
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/character/:id",
      "dest": "./src/routes/index.js"
    },
    {
      "src": "/login",
      "dest": "./src/routes/index.js"
    },
    {
      "src": "/fav",
      "dest": "./src/routes/index.js"
    },
    {
      "src": "/fav/:id",
      "dest": "./src/routes/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./src/routes/index.js"
    }
  ]
}
